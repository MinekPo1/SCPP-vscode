{
    "name": "scpp-vscode",
    "displayName": "SCPP for vscode",
    "publisher": "MinekPo1",
    "description": "SCPP support for vscode",
    "repository": "https://github.com/MinekPo1/SCPP-vscode",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "scpp",
            "aliases": ["SCPP", "scpp"],
            "extensions": [".sc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "scpp",
            "scopeName": "source.scpp",
            "path": "./syntaxes/scpp.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "scpp",
                "path": "./snippets/scpp.json"
            }
        ]
    }
}
